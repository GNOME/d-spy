[Desktop Entry]
Categories=GNOME;GTK;Development;
Exec=d-spy
Icon=@app_id@
Name=D-Spy
Comment=Discover and explore D-Bus services
Keywords=dspy;dbus;
DBusActivatable=true
StartupNotify=true
Terminal=false
Type=Application
X-Flatpak-RenamedFrom=d-spy
Actions=new-window;

[Desktop Action new-window]
Exec=d-spy --new-window
Name=New Window
